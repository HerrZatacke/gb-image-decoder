{
  "name": "gb-image-decoder",
  "version": "2.0.2",
  "description": "Decoder classes for GameBoy-encoded images",
  "repository": "",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "npm run eslint && npm run test && npm run build",
    "build": "unbuild",
    "dev": "vitest dev",
    "test:clean": "rimraf images/*",
    "test": "npm run test:clean && vitest run",
    "test:u": "npm run test:clean && vitest run -u",
    "eslint": "eslint -c .eslintrc --ext .js,.ts ."
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "canvas": "^3.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jsdom": "^26.1.0",
    "ohash": "^2.0.11",
    "typescript": "^5.4.5",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "ohash": "^2.0.11"
  }
}
